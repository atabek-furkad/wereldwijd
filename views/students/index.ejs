<h2>Search Students</h2>
<form action="/students" method="GET">
  <div>
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>" />
  </div>
  <div>
    <label>Country</label>
    <input type="text" name="country" value="<%= searchOptions.country %>" />
  </div>
  <button type="submit">Search</button>
</form>

<% students.forEach(student => { %>
<div>
  <%= student.name%> <%= student.birthDate %> <%= student.preferredLanguage %>
  <%= student.country %>
  <div>
    <% if (student.coverImagePath != null) { %>
    <img
      height="200"
      width="150"
      style="object-fit: cover"
      src="<%= student.coverImagePath %>"
      alt=""
    />
    <% } %>
  </div>

  <a href="/students/<%= student.id %>">View</a>

  <a href="/students/<%= student.id %>/edit">Edit</a>

  <%- include("../partials/deleteForm.ejs", { url: `/students/${student.id}` })
  %>
</div>
<br />
<br />
<% }) %>
